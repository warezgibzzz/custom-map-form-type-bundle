{% block custom_map_widget %}
    <div id="{{ id }}" class="{{ attr.class }}" style="width: {{ width }}px; height: {{ height }}px; overflow: hidden">
        <img src="{{ parameters.image }}" style="margin-left: -640px">
    </div>

    {% for child in form %}
        {% if child.vars.value is empty %}
            {{ form_row(child, {
                value: default[ child.vars.name ]
            }) }}
        {% else %}
            {{ form_row(child) }}
        {% endif %}
    {% endfor %}

    <script>
        var {{ name }}CustomMap = new CustomMapWidgetClass('{{ id }}', {{ parameters | json_encode | raw }});
    </script>
{% endblock %}
